                <script>
                    $(function () {
                        var result;
                        $.ajax({
                            type: "GET",
                            url: "/login/getcookie",
                            dataType: 'text',
                            async: false,
                            data: {},
                            success: function (data) {
                                result = data;
                                if (result) {
                                    $('#login').hide();
                                    $('#reg').hide();
                                    $('#logout').text(result+" logout ");
                                } else {
                                    $('#logout').hide();
                                }
                            }
                        });
                    });
                    function logoutFuc() {

                        var result;
                        $.ajax({
                            type: "GET",
                            url: "/logout",
                            dataType: 'text',
                            async: false,
                            data: {},
                            success: function (data) {
                                result = data;
                            }
                        });
                        if (result === "true") {
                            location.reload();
                        }
                    }
                </script>
